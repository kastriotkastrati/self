    "sideBar.background": "#171717",
    "sideBar.border": "#171717",
    "sideBarSectionHeader.border": "#171717",
    "sideBarSectionHeader.foreground": "#aaa",
    "sideBarSectionHeader.background": "#171717",
    "sideBarTitle.foreground": "#aaa",
    "tab.inactiveBackground": "#171717",
    "tab.activeBackground": "#020202",
    "tab.border": "#171717",
    "tab.lastPinnedBorder": "#171717",
    "tab.activeBorder": "#171717",
    "titleBar.activeBackground": "#171717",
    "titleBar.inactiveBackground": "#171717",
    "titleBar.activeForeground": "#aaa",
    "titleBar.border": "#171717",
    "list.hoverBackground": "#2E3339",
    "list.hoverForeground": "#aaa",
    "list.activeSelectionBackground": "#2E3339",
