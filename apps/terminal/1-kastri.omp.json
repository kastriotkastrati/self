{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#f5f5f5",
          "style": "plain",
          "template": "\ufb26 ",
          "type": "text"
        },
        {
          "foreground": "#e8e8e8",
          "properties": {
            "style": "agnoster"
          },
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#e8e8e8",
          "style": "plain",
          "template": " <#e8e8e8>git:</>{{ .HEAD }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}
